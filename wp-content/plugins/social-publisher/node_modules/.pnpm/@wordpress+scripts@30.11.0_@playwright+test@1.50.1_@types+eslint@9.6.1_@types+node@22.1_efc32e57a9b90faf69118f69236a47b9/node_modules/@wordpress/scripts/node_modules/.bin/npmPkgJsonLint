#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules/npm-package-json-lint/dist/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules/npm-package-json-lint/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules/npm-package-json-lint/dist/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules/npm-package-json-lint/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules/npm-package-json-lint/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../npm-package-json-lint@6.4.0_typescript@5.7.3/node_modules/npm-package-json-lint/dist/cli.js" "$@"
fi
