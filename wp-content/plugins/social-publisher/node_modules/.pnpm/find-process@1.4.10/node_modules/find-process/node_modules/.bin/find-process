#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/find-process@1.4.10/node_modules/find-process/bin/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/find-process@1.4.10/node_modules/find-process/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/find-process@1.4.10/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/find-process@1.4.10/node_modules/find-process/bin/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/find-process@1.4.10/node_modules/find-process/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/find-process@1.4.10/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/find-process.js" "$@"
else
  exec node  "$basedir/../../bin/find-process.js" "$@"
fi
