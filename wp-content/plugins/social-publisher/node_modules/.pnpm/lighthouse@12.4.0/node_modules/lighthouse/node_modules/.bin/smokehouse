#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/test/smokehouse/frontends/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/test/smokehouse/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/test/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/test/smokehouse/frontends/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/test/smokehouse/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/test/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/cli/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules/lighthouse/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/lighthouse@12.4.0/node_modules:/Users/steffenmuller/Local Sites/et-website/app/public/wp-content/plugins/social-publisher/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/test/smokehouse/frontends/smokehouse-bin.js" "$@"
else
  exec node  "$basedir/../../cli/test/smokehouse/frontends/smokehouse-bin.js" "$@"
fi
